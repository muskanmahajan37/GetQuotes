{
  "name": "Get Inspired!",
   "author": "Vinit Shahdeo",
  "version": "1.1",
  "description": "Get a new quote each minutes! It is just to inspire you.",
  "icons": { "128": "quotes_icon.png" },
    
    "background": {
    "persistent": true,
    "scripts": ["jquery.js","script.js","test.js"]
  },
  "browser_action": {
    "default_title": "Get Inspired!",
    "default_icon": "quotes_action.png",
    "default_popup": "index.html"
  },
    
  "permissions": [
     "webRequestBlocking",
     "webRequest",
     "activeTab",
     "tabs",
     "http://*/*",
     "https://*/*"
  ],
  "manifest_version": 2
}
